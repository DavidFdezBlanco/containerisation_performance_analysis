Host khamul
     Hostname khamul.citi.insa-lyon.fr
     User amirekik
     ForwardAgent yes
     StrictHostKeyChecking no
     UserKnownHostsFile=/dev/null
     RequestTTY force
     ServerAliveInterval 30
     ServerAliveCountMax 6
     IdentityFile /home/th3gary/.ssh/config_key

Host device110
     Hostname 10.0.1.110
     User amirekik
     ProxyCommand ssh -W %h:%p khamul
     IdentityFile /home/th3gary/.ssh/config_key
	 
